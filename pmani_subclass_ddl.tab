Create Table Subclass(
     Dept number(10,0) NOT NULL ENABLE,
    Class NUMBER(4,0) NOT NULL ENABLE,
    Subclass NUMBER(4,0) NOT NULL ENABLE,
    Subclass_Name VARCHAR2(30) NOT NULL ENABLE,
    CONSTRAINT "FK_DEPT_SUBCLASS" FOREIGN KEY (DEPT) REFERENCES DEPS(DEPT),
    CONSTRAINT "FK_CLASS_SUBCLASS" FOREIGN KEY (Class) REFERENCES Class(Class),
    CONSTRAINT "UQ_SUBCLASS" UNIQUE (DEPT,Class,Subclass)
)
/