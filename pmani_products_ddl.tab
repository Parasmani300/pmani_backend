Create Table Products(
    SKU NUMBER(8,0) NOT NULL ENABLE,
    SKU_DESC VARCHAR2(50) NOT NULL ENABLE,
    SHORT_DESC VARCHAR2(50) NOT NULL ENABLE,
    Dept number(10,0),
    Class NUMBER(4,0),
    Subclass NUMBER(4,0),
    CONSTRAINT "PK_PRODUCTS" PRIMARY KEY (SKU),
    CONSTRAINT "FK_SUBCLASS_SUBCLASS" FOREIGN KEY (Dept,CLass,Subclass) REFERENCES Subclass(Dept,CLass,Subclass),
    CONSTRAINT "FK_SKU_UPC" FOREIGN KEY (SKU) REFERENCES UPC_EAN(SKU),
    Last_updated DATE DEFAULT SYSDATE NOT NULL ENABLE
)
/
Create Table Product_Attributes(
    SKU NUMBER(8,0) NOT NULL ENABLE,
    SKU_NOTES VARCHAR2(50),
    Rating VARCHAR2(6),
    COLOR VARCHAR2(20),
    Product_SIZE VARCHAR2(20),
    SKU_STATUS VARCHAR2(20),
    STREET_DATE DATE,
    IN_STOCK_DATE DATE,
    OUT_STOCK_DATE DATE,
    MAP NUMBER(4,0),
    MAP_START_DATE DATE,
    MAP_END_DATE DATE,
    PERFORMANCE_CODE VARCHAR2(20),
    URL_SMALL VARCHAR2(255),
    URL_BIG VARCHAR2(255),
    LAST_UPDATED_DATE DATE DEFAULT SYSDATE NOT NULL ENABLE,
    CONSTRAINT "FK_SKU_PRODUCT_ATTRIBUTES" FOREIGN KEY (SKU) REFERENCES Products(SKU),
    CONSTRAINT "PK_PRODUCT_ATTRIBUTES" PRIMARY KEY (SKU)
)
/